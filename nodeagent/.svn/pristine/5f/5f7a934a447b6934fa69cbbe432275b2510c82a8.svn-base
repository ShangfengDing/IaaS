package appcloud.nodeagent.info;

/**
 * 磁盘读写速率，以k为单位
 * @author wylazy
 *
 */
public class DiskSpeed {

	private float diskReadK;
	private float diskWriteK;
	
	public DiskSpeed(float diskReadK, float diskWriteK) {
		super();
		this.diskReadK = diskReadK;
		this.diskWriteK = diskWriteK;
	}
	
	/**
	 * 
	 * @return 磁盘读速率(k/s)
	 */
	public float getDiskReadK() {
		return diskReadK;
	}
	
	public void setDiskReadK(float diskReadK) {
		this.diskReadK = diskReadK;
	}
	
	/**
	 * 
	 * @return 磁盘写速率(k/s)
	 */
	public float getDiskWriteK() {
		return diskWriteK;
	}
	
	public void setDiskWriteK(float diskWriteK) {
		this.diskWriteK = diskWriteK;
	}
	
	@Override
	public String toString() {
		return "DiskSpeed [diskReadK=" + diskReadK + ", diskWriteK="
				+ diskWriteK + "]";
	}

}
