<!DOCTYPE html>
<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<%@ taglib prefix="s" uri="/struts-tags" %>
<div class="modal-body">
    <div class="form-group">
        <label for="name">名称</label>
        <p class="form-control" id="name">${param.name}</p>
    </div>
    <div class="form-group">
        <label for="code">密码</label>
        <p class="form-control" id="code">${param.code}</p>
    </div>
    <div class="form-group">
        <label for="software">软件</label>
        <p class="form-control" id="software">${param.software}</p>
    </div>
    <div class="form-group">
        <label for="description">描述</label>
        <p class="form-control" id="description">${param.description}</p>
    </div>
</div>
