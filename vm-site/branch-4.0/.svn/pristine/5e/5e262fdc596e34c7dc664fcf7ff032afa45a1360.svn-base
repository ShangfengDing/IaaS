@CHARSET "UTF-8";
	a.instruction:link{text-decoration: underline;color: #d4a639}
    a.instruction:visited{text-decoration: none;color:#d4a639}
    a.instruction:hover{text-decoration: none;color:#d4a639}
    a.instruction:active{text-decoration: none;color: #d4a639}
    .instruction {
        margin-left: 20px;
        display: inline-block;
        margin-top: 5px;
    }
    @media screen and (min-width: 992px) {
        #button-gutter{
            margin-left:10px;
        }
       
        #disk{
            margin-left: -30px;
        }
        .label-pos{
            position: absolute;
            right: 25px;
        }
        .label-pos2{
            position: absolute;
            right: 10px;
        }
        .fix-gutter{
            margin-left:7px;
        }
         .content-gutter{
        margin-left: -20px;
    }
        .left-card{
        margin-left:10px;
        }
        .long-label{
        margin-left:-17px; 
        }
        
        .button-group-align{
        margin-left:-4px;  
        }
    }
    @media(max-width: 991px){
        #button-gutter{
            margin-top:10px;
        }
        #disk{
            margin-top:10px;
          
        }
       
     
    
    }
    .glyphicon-edit{
        padding-left: 10px;
    }
    
    
    .blank-card{
        padding-bottom:141px;
    }
   
    .content-edit{
        display: inline-block;
    }
    .right-label{
        margin-bottom: 10px;
    }
    .card-gutter{
        margin-bottom:13px;
    }
    
    @media (max-width: 767px) {
        .nav-avatar-menu > .arrow {
            right: 10px;
        }
        .mobile-margin-right-15 {
            margin-right: 15px;
        }
    }
    .table>tbody>tr>td{
        border-bottom: 0;
        border-top: 0;
        font-size:14px ;
    }
    .front-table>tbody>tr>td:last-child{
        border-bottom: 0;
    }
    .but_back{
        background: transparent;
        border: 0;
    }
    .table_header{
        background-color: rgba(86,61,124,.15);
        color: black
    }
    .label_sum {
        font-size: 30px;
        color: black;
    }
    .head_line{
        width: 3px;
        height: 20px;
        background: #3778bc;
        vertical-align: middle
    }
	.drop_menu{
		min-width:0;
	}
