public class ModelUtil {

    /**
     * 判断java bean是否为null
     * @param o
     * @return
     */
    public static Boolean isEmpty(Object o) {
        return o==null;
    }

    public static Boolean isNotEmpty(Object o) {
        return !isEmpty(o);
    }

}
