public class NumUtil {

    /**
     * 是否为空
     * @param lo
     * @return
     */
    public static Boolean isEmpty(Long lo) {
        return (lo==null)||(lo==0);
    }

    public static Boolean isNotEmpty(Long lo) {
        return (lo==null)||(lo==0);
    }
}
