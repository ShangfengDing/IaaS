package com.distributed.common.constant;

/**
 * Created by Idan on 2017/12/21.
 */
public class EnumConstants {

    /**
     * 存活的状态只有两种
     */
    public static enum SurvivalStatus {
        ALIVE,
        DEAD;

        public String toString() {
            switch (this) {
                case ALIVE:
                    return "ALIVE";
                case DEAD:
                    return "DEAD";
                default:
                    return "ALIVE";
            }
        }
    }

    public static enum DataCenterType {
        SOURCE,
        DEST;

        public String toString() {
            switch (this) {
                case SOURCE:
                    return "SOURCE";
                case DEST:
                    return "DEST";
                default:
                    return "DEST";
            }
        }
    }

    public static enum HostType {
        FUNCTION_NODE, COMPUTE_NODE;

        public String toString() {
            switch (this) {
                case FUNCTION_NODE:
                    return "FUNCTION_NODE";
                case COMPUTE_NODE:
                    return "COMPUTE_NODE";
                default:
                    return "FUNCTION_NODE";
            }
        }

    }

}
